<h2 class="mt-5 ml-3">Add to the map</h2>
  <div class="form-bottom">
    <%= simple_form_for(@point_of_interest) do |f| %>
    <div class="form-inputs">
      <%= f.input :photo %>
      <%= f.input :poi_type, label: "What is it?", collection: POINT_OF_INTEREST %>
      <%= f.input :description %>
   </div>
   <div class="form-actions">
     <%= f.submit "Add" %>
   </div>
     <% end %>
  </div>
